<h1>Listing sleeves</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sleeves.each do |sleefe| %>
      <tr>
        <td><%= sleefe.description %></td>
        <td><%= link_to 'Show', sleefe %></td>
        <td><%= link_to 'Edit', edit_sleefe_path(sleefe) %></td>
        <td><%= link_to 'Destroy', sleefe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sleeve', new_sleefe_path %>
